<!DOCTYPE html>
<html lang="es">
  <head>
    <title>WCI</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <main>
      <% if logged_in? %>
        <%= render('layouts/nav') %>
        <%= render('layouts/breadcrumbs') %>
      <%end%>
      <div class="container-fluid">
        <%= yield %>
      </div>
    </main>
  </body>
</html>
